.main
  display: inline-flex
  align-items center
  justify-content center
  margin 10px
  border 3px solid black
  font-family HelveticaNeue, Helvetica, Arial, sans-serif
  padding 2px 12px
  min-height 35px
  user-select none
  cursor: pointer
  color #111
  text-decoration none
  font-weight 600
  &:hover
    background-color black
    color #ddd